{% extends 'base.html.twig' %}

{% block title %}Service{% endblock %}

{% block body %}
    <h1>Service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>ServiceId</th>
                <td>{{ service.serviceId }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ service.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ service.description }}</td>
            </tr>
            <tr>
                <th>Expertise</th>
                <td>{{ service.expertise }}</td>
            </tr>
            <tr>
                <th>DureeJours</th>
                <td>{{ service.dureeJours }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ service.prix }}</td>
            </tr>
            <tr>
                <th>ModePaiement</th>
                <td>{{ service.modePaiement }}</td>
            </tr>
            <tr>
                <th>CreeLe</th>
                <td>{{ service.creeLe ? service.creeLe|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>MisAJourLe</th>
                <td>{{ service.misAJourLe ? service.misAJourLe|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_service_index') }}">back to list</a>

    <a href="{{ path('app_service_edit', {'serviceId': service.serviceId}) }}">edit</a>

    {{ include('service/_delete_form.html.twig') }}
{% endblock %}
